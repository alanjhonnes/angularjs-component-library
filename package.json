{
  "name": "angularjs-component-library",
  "version": "0.0.0",
  "description": "Base template for angularjs library",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --color --mode development --config webpack.config.development.js --disable-host-check --host 0.0.0.0",
    "prebuild": "rimraf ./dist/*",
    "build": "webpack --color --mode production --config webpack.config.development.js",
    "build:dev": "webpack --color --config webpack.config.development.js",
    "test": "jest",
    "lint": "eslint ./",
    "lint-rule-timings": "cross-env TIMING=1 yarn lint",
    "commit": "git-cz",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Alan Jhonnes <alanjhonnes@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@storybook/addon-essentials": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/cli": "^5.3.18",
    "@storybook/html": "^5.3.18",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "extract-css-chunks-webpack-plugin": "^4.7.4",
    "fork-ts-checker-notifier-webpack-plugin": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "storybook-addon-angularjs": "^1.3.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.3",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/angular": "^1.7.0",
    "angular": "^1.7.9",
    "node-sass": "^4.14.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
